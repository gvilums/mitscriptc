mod = fun(a, b){
	return a - ((a/b)*b);
};

random = fun(seed, lim){ //pseudo random generator
	this = {
		cur:seed;
		val: fun(){
			this.cur = mod((this.cur * 6829 + 33), lim);
			return this.cur;
		};
		
	};
	return this;
};

List = fun(){
	this = {
		head:None;
		plus:fun(vl){
			this.head = {car:vl; cdr:this.head;};
		};
		forall:fun(f){
			t = this.head;
			while(!(t == None)){
				f(t.car);
				t = t.cdr;
			}
		};	
	};
	return this;
};

l = List();
i = 0;
mn = 10000;
ran = random(42, 10000);
min = fun(x){
	global mn;
	if(x < mn){
		mn = x;
	}
};
while(i < 200){
	l.plus(ran.val());
	i = i + 1;
	l.forall(min);
	print(mn);
}







