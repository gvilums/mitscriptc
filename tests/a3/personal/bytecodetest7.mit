pat = fun(n, m){
	mt = {};
	i = 0;
	while(i < n){
		j = 0;
		mt[i] = {};
		while(j < m){
			if ((((i + j)/2)*2) == i+j){
				mt[i][j] = "/";
			}
			else {
				mt[i][j] = " ";
			}
			j = j + 1;
		}
		i = i + 1;
	}
	return mt;
};

prt = fun(n, m){
	p = pat(n, m);
	
	i = 0;
	while(i < n){
		j = 0;
		str = "";
		while(j < m){
			str = str + p[i][j];
			j = j + 1;
		}
		print(str);
		i = i + 1;
	}
	
};

prt(800, 80);
